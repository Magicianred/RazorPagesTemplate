@page
@model LockoutModel
@inject IStringLocalizer<SharedResource> sharedLoc
@{
        ViewData["Title"] = sharedLoc["Locked out"];
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">@sharedLoc["This account has been locked out, please try again later"].</p>
</header>
